rule " presence-change-text.groovy"

   device presence : presenceSensor;
   output send : string;
   
   prsence.prsence[.~>];
   
   prsence.prsence[.~>prsent];
   true;
   send("I arrived at the designated place")
   
   prsence.prsence[.~>not prsent];
   true;
   send("I left at the designated place")
end
