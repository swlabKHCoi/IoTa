rule "has-barkley-been-fed"

    device feeder1 : contact;
    input phone1 : phone;
    input time1 : time;
    output send : string;

      rule "feeder open"
        
        feeder1.contact [ .~>open ];
	time1 == true;
	feeder1 := open;

      end

      rule "feeder close"

        contactSensor.contact [ .~>closed ];
	time1 == true && feeder1 == closed;
	send ("feeder is closed");

      end

end
	
